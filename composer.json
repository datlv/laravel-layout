{
    "name": "datlv/laravel-layout",
    "description": "Laravel Layout Package",
    "keywords": [
        "datlv",
        "laravel",
        "layout"
    ],
    "homepage": "https://github.com/datlv/laravel-layout",
    "license": "MIT",
    "authors": [
        {
            "name": "Datlv",
            "email": "datle2011@gmail.com",
            "homepage": "http://Datlv.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravelcollective/html": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Datlv\\Layout\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Datlv\\Layout\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datlv\\Layout\\ServiceProvider"
            ]
        }
    }
}
